{
    "content_scripts": [
      {
        "js": ["background.js"],
        "matches": ["https://www.saveonfoods.com/*"]
      }
    ],
    "name": "Diabetic Helper",
    "version": "1.1.0",
    "description": "Identifies low sugar foods",
    "manifest_version": 3,
    "author": "Reese Ochitwa",
    "permissions": ["https://www.saveonfood.com/*"],
    "action":{
        "default_popup": "index.html",
        "default_title": "Diabetic Helper"
    }
}

